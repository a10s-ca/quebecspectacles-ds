{
  "@type": "Place",
  "@id": "{{ url de la page pour le lieu }}",
  "url": "{{ url de la page pour le lieu }}",
  "address": {
    "@type": "PostalAddress",
    "addressLocality": "{{ address_locality }}",
    "addressRegion": "{{ address_region }}",
    "postalCode": "{{ postal_code }}",
    "streetAddress": "{{ street_address}}",
    "adresseCountry": "{{ address_country }}"
  },
  "telephone": "{{ telephone }}",
  "sameAs": [
    "{{ sameas_website }}",
    "{{ sameas_wikidata }}"
  ],
  "name": "{{ name }}",
  "geo": {
    "type": "http://schema.org/GeoCoordinates",
    "latitude": "{{ geo.latitude }}",
    "longitude": "{{ geo.longitude }}"
  },
  "image": ["{{ images de la location }}"],
  "description": "{{ description }}"
}
